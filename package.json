{
  "name": "backend-template-reborn",
  "version": "1.0.0",
  "main": "dist/server.js",
  "license": "ISC",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "tsx watch --env-file=.env src/server.ts",
    "build": "tsup src/server.ts src/worker.ts --format cjs --clean",
    "start:dev": "npm run dev",
    "start:prod": "node --env-file=.env dist/server.js",
    "start:worker": "tsx watch --env-file=.env src/worker.ts",
    "start:worker:prod": "node --env-file=.env dist/worker.js",
    "format": "prettier --write \"src/**/*.{ts,js}\"",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "prisma:seed": "tsx prisma/seed.ts"
  },
  "prisma": {
    "seed": "npm run prisma:seed"
  },
  "dependencies": {
    "@fastify/cors": "11.1.0",
    "@fastify/jwt": "10.0.0",
    "@prisma/client": "6.14.0",
    "@sentry/node": "10.5.0",
    "@sentry/profiling-node": "10.5.0",
    "bcryptjs": "3.0.2",
    "bullmq": "5.66.0",
    "cpf-cnpj-validator": "1.0.3",
    "fastify": "5.5.0",
    "ioredis": "5.8.2",
    "nodemailer": "7.0.5",
    "pino": "9.9.0",
    "uuid": "11.1.0",
    "zod": "4.0.17"
  },
  "devDependencies": {
    "@eslint/js": "9.33.0",
    "@types/node": "24.3.0",
    "@types/nodemailer": "7.0.0",
    "eslint": "9.33.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "pino-pretty": "13.1.1",
    "prettier": "3.6.2",
    "prisma": "6.14.0",
    "ts-jest": "29.4.1",
    "tsup": "8.5.0",
    "tsx": "4.20.4",
    "typescript": "5.9.2",
    "typescript-eslint": "8.40.0"
  }
}